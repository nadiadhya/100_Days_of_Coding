public class MembuatFungsi {

    public static int fungsiJumlah(int a, int b) {
        return a + b;
    }

    public static void main(String[] args) {
        // Calling the static fungsiJumlah() method
        int hasil = fungsiJumlah(14, 28);

        // Displaying the sum
        System.out.println("Hasil = " + hasil);
    }
}
