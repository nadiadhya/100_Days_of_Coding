public class SetIndexElement {
    public static void main(String[] args) {
        //Membuat Array
        int[] array = {1, 2, 3, 4, 5};
        
        //Set nilai pada index tertentu
        array[3] = 14;

        //Menampilkan array
        for (int i = 0; i < array.length; i++){
            System.out.println(array[i]);
        }
    }
}
