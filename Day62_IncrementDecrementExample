public class IncrementDecrementExample {
    public static void main(String[] args) {
        // Contoh penggunaan increment (++)
        int nilaiAwal = 5;
        System.out.println("Nilai awal: " + nilaiAwal);

        // Increment nilaiAwal
        nilaiAwal++;
        System.out.println("Setelah increment: " + nilaiAwal);

        // Contoh penggunaan decrement (--)
        int nilaiLain = 8;
        System.out.println("Nilai lain: " + nilaiLain);

        // Decrement nilaiLain
        nilaiLain--;
        System.out.println("Setelah decrement: " + nilaiLain);
    }
}
